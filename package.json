{
  "name": "skysurfers",
  "version": "1.0.0",
  "main": "build/src/server.js",
  "description": "",
  "scripts": {
    "start": "electron ./build/src/server.js",
    "build": "tsc && gcc main.c -o main -lws2_32",
    "dist": "electron-builder",
    "electron-pack": "build --em.main=build/src/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "electron-builder": "^23.6.0",
    "electron": "^21.1.1"
  },
  "dependencies": {
    "concurrently": "^7.4.0",
    "dayjs": "^1.11.5",
    "express": "^4.18.2",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "socket.io": "^4.5.3",
    "wait-on": "^6.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.skysurfers.skysurfers",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "productName": "Sky Surfers",
    "target": "NSIS",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
}
